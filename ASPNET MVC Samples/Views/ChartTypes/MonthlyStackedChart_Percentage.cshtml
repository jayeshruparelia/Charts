
@{
    ViewBag.Title = "Magarpatta City Pune";
}

<div id="chartContainer"></div>

<script type="text/javascript">

    $(function () {
        var chart = new CanvasJS.Chart("chartContainer", {
            animationEnabled: true,
            title: {
                text: "Monthly CO2 emission (%)"
            },
            axisY: {
                suffix: "%"
            },
            valueFormatString: "#0.",

            dataPointWidth: 65,

            data: [
                {
                    type: "stackedColumn100",
                    legendText: "Unutilized",
                    indexLabel: "#percent %",
                    indexLabelPlacement: "inside",
                    indexLabelFontColor: "white",
                    showInLegend: true,
                    bevelEnabled: true,
                    dataPoints: [
                        { y: 40, x: new Date(2010, 1) },
                        { y: 50, x: new Date(2010, 2) },
                        { y: 60, x: new Date(2010, 3) },
                        { y: 60, x: new Date(2010, 4) },
                        { y: 61, x: new Date(2010, 5) },
                        { y: 63, x: new Date(2010, 6) },
                        { y: 65, x: new Date(2010, 7) },
                        { y: 45, x: new Date(2010, 8) },
                        { y: 65, x: new Date(2010, 9) },
                        { y: 38, x: new Date(2010, 10) },
                        { y: 42, x: new Date(2010, 11) },
                        { y: 55, x: new Date(2010, 12) }
                    ]
                },
                {
                    type: "stackedColumn100",
                    legendText: "Wasted",
                    indexLabel: "#percent %",
                    indexLabelPlacement: "inside",
                    indexLabelFontColor: "white",
                    showInLegend: true,
                    bevelEnabled: true,
                    dataPoints: [
                        { y: 28, x: new Date(2010, 1) },
                        { y: 18, x: new Date(2010, 2) },
                        { y: 12, x: new Date(2010, 3) },
                        { y: 10, x: new Date(2010, 4) },
                        { y: 10, x: new Date(2010, 5) },
                        { y: 7, x: new Date(2010, 6) },
                        { y: 5, x: new Date(2010, 7) },
                        { y: 4.5, x: new Date(2010, 8) },
                        { y: 13, x: new Date(2010, 9) },
                        { y: 29, x: new Date(2010, 10) },
                        { y: 44, x: new Date(2010, 11) },
                        { y: 30, x: new Date(2010, 12) }
                    ]
                }]
        });
        chart.render();
    });
</script>